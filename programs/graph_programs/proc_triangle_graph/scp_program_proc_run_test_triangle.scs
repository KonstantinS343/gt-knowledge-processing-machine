=> nrel_initiation_condition_and_result: (..scp_program_proc_run_test_triangle_initiation_condition
					  => ..scp_program_proc_run_test_triangle_result2);

<= nrel_scp_program_key_sc_elements: 
	{
		concept_graph;
		concept_triangle_graph;
		rrel_vertex;
		rrel_oredge
	};

<- key_sc_element:
	..scp_program_proc_run_test_triangle_behavior
	(*
		<- sc_description_of_scp_program_behavior;;
		<= nrel_sc_text_translation:
		...
		(*
			-> rrel_example:
				[Это программа создает из понятия "треугольный граф" к графу либо дугу приндлежности, если граф треугольный, либо отрицательную дугу, если граф нетреугольный];;
		*);;
	*);;

..scp_program_proc_run_test_triangle_initiation_condition
= [*
	concept_graph _-> _curr_graph;;
*];;


..scp_program_proc_run_test_triangle_result1
= [*
	concept_graph _-> _curr_graph;;
	concept_triangle_graph _-> _curr_graph;;
*];;

..scp_program_proc_run_test_triangle_result2
= [*
	concept_graph _-> _curr_graph;;
	concept_triangle_graph _-|> _curr_graph;;
