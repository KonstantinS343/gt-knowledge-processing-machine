ui_menu_search_operation_planar_graph =>ui_nrel_command_template:
[*
	question_of_checking_planar_graph _-> ._question_of_checking_planar_graph_instance
	(*
		_-> ui_arg_1;;
	*);;

	._question_of_checking_planar_graph_instance _<- question;;
*];;

