sc_node_not_relation -> set_exception
	(*
		-> nrel_system_idtf;;
		-> nrel_main_idtf;;
		-> sc_type_node_struct;;
	*);;

set_exception => nrel_main_idtf:
	[множество исключений]
		(*<-lang_ru;;*);
	[set of exception]
		(*<-lang_en;;*);;

sc_type_node_struct -> input_graph;;

input_graph => nrel_main_idtf:
	[Новый граф]
		(*<-lang_ru;;*);
	[new graph]
		(*<-lang_en;;*);;
